#!/usr/bin/env bash
#Write a Bash script that displays the time for 12 hours and 59 minutes:
#display hours from 0 to 12
#display minutes from 1 to 59



hour=0
echo "Hour: $hour"
while [ $hour -le 12 ];

do
min=1
	while [ $min -lt 60 ];
	      do
     	       echo $min
	       (( min=min+1 ))
	       if [ $min -eq 60 ]
	       then
		   (( hour=hour+1 ))
		   echo "Hour: $hour"
               fi
		  
	      done

done
